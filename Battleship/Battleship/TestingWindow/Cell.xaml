<UserControl x:Class="Battleship.TestingWindow.Cell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Battleship.TestingWindow"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <local:CellCodeToFillConverter x:Key="CellCodeToFill"/>
    </UserControl.Resources>
    <Border Background="Aqua" BorderBrush="Black" BorderThickness="1" MouseLeftButtonUp="UIElement_OnMouseLeftButtonUp" MouseRightButtonUp="UIElement_OnMouseRightButtonUp">
        <!--<Ellipse  Fill="{Binding AttackCode, Converter={StaticResource CellCodeToFill}}"/>-->
    </Border>
</UserControl>
